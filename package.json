{
  "name": "poc",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "serve": "vue-cli-service serve",
    "build": "vue-cli-service build --mode prod",
    "lint": "vue-cli-service lint",
    "start": "node app.js",
    "dev": "vue-cli-service serve --mode dev",
    "prod": "vue-cli-service serve --mode prod"
  },
  "dependencies": {
    "axios": "^0.19.0",
    "body-parser": "^1.19.0",
    "bootstrap": "^4.3.1",
    "bootstrap-vue": "^2.0.4",
    "cookie-parser": "^1.4.4",
    "core-js": "^2.6.5",
    "cors": "^2.8.5",
    "cryptr": "^6.0.1",
    "ejs": "^2.6.2",
    "express": "^4.17.1",
    "express-session": "^1.17.0",
    "jquery": "^3.4.1",
    "mdbvue": "^5.5.1",
    "mongodb": "^3.2.7",
    "mongoose": "^5.6.3",
    "nodemon": "^1.19.4",
    "passport": "^0.4.0",
    "passport-google-oauth": "^2.0.0",
    "path": "^0.12.7",
    "scss": "^0.2.4",
    "serve-static": "^1.14.1",
    "vee-validate": "^2.2.13",
    "vue": "^2.6.10",
    "vue-particles": "^1.0.9",
    "vue-router": "^3.0.3"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^3.8.0",
    "@vue/cli-plugin-eslint": "^3.8.0",
    "@vue/cli-service": "^4.1.1",
    "babel-eslint": "^10.0.1",
    "dotenv": "^8.0.0",
    "eslint": "^5.16.0",
    "eslint-plugin-vue": "^5.0.0",
    "node-sass": "^4.13.0",
    "sass-loader": "^8.0.0",
    "vue-template-compiler": "^2.6.10",
    "webpack": "^4.41.2"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "node": true
    },
    "extends": [
      "plugin:vue/essential",
      "eslint:recommended"
    ],
    "rules": {
      "no-console": [
        "off"
      ],
      "no-debugger": [
        "off"
      ],
      "no-unused-vars": [
        "off"
      ],
      "vue/no-unused-components": [
        "error",
        {
          "ignoreWhenBindingPresent": true
        }
      ]
    },
    "parserOptions": {
      "parser": "babel-eslint"
    }
  },
  "postcss": {
    "plugins": {
      "autoprefixer": {}
    }
  },
  "browserslist": [
    "> 1%",
    "last 2 versions"
  ]
}
